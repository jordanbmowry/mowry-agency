{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "[ \"$CI\" = \"true\" ] || (node -e \"try { require('simple-git-hooks').install() } catch (e) { }\" 2>/dev/null || true)",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "type-check": "nuxt typecheck",
    "ci": "biome ci .",
    "db:types": "cd ../mowry_leads_database && npm run sync-types",
    "db:migrate": "cd ../mowry_leads_database && npm run migrate",
    "db:reset": "cd ../mowry_leads_database && npm run reset",
    "db:start": "cd ../mowry_leads_database && npm run start",
    "db:stop": "cd ../mowry_leads_database && npm run stop",
    "db:status": "cd ../mowry_leads_database && npm run db:status"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@headlessui/vue": "^1.7.23",
    "@internationalized/date": "^3.10.0",
    "@netlify/nuxt": "^0.1.19",
    "@nuxt/content": "^3.7.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^2.0.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/ui": "^4.0.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/seo": "^3.2.2",
    "@nuxtjs/supabase": "^2.0.1",
    "@radix-icons/vue": "^1.0.0",
    "@supabase/supabase-js": "^2.74.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/nodemailer": "^7.0.2",
    "@vue-email/components": "^0.0.21",
    "@vue-email/nuxt": "^0.8.19",
    "@vue-email/render": "^0.0.9",
    "@vue-email/tailwind": "^0.2.0",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "feed": "^5.1.0",
    "joi": "^18.0.1",
    "nodemailer": "^7.0.9",
    "nuxt": "^4.1.2",
    "pg": "^8.16.3",
    "radix-vue": "^1.9.8",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^2.5.5",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue3-simple-typeahead": "^1.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@iconify-json/heroicons": "^1.2.3",
    "@nuxt/test-utils": "^3.19.2",
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@types/joi": "^17.2.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^18.0.1",
    "lint-staged": "^16.2.6",
    "playwright-core": "^1.55.1",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4.1.15",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue,json,md}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  }
}
